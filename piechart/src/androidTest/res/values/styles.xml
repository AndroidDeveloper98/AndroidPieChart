<resources>

  <style name="ScreenshotTestAppTheme" parent="Theme.AppCompat">
    <item name="android:windowBackground">#aaa</item>
    <item name="android:colorBackground">#aaa</item>
  </style>

  <style name="TestStyleNotDefined">
  </style>

  <style name="TestStyleEmptyAttribute">
    <item name="testColorArrayAttr"/>
  </style>

  <style name="TestStyleAtNullAttribute">
    <item name="testColorArrayAttr">@null</item>
  </style>

  <style name="TestStyleLiteralValue">
    <item name="testColorArrayAttr">#ff0</item>
  </style>

  <style name="TestStyleSingleReference">
    <item name="testColorArrayAttr">@color/testColorArraySingle</item>
  </style>

  <style name="TestStyleArrayReference">
    <item name="testColorArrayAttr">@array/testColorArray</item>
  </style>

</resources>
